	<queryString>
		<![CDATA[SELECT a.ENCODED_KEY,
         a.account_state,
         a.id,
       b.DUE_DATE AS dueDate,
       b.REPAID_DATE AS repaidDate,
       b.PRINCIPAL_EXP AS principalExp,
       b.PRINCIPAL_PAID AS principalPaid,
       b.PRINCIPAL_DUE AS principalDue,
       b.INTEREST_EXP AS interestExp,
       b.INTEREST_PAID AS interestPaid,
       b.INTEREST_DUE AS interestDue,
       b.FEE_EXP AS feeExp,
